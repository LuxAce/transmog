<?xml version="1.0" encoding="UTF-8"?>
<structure>
  <name>TEI encoding for Modern Library Bibliography</name>
  <description />
  <transformation>modern-library-tei.xsl</transformation>
  <rootNodeTypeId>TEI</rootNodeTypeId>
  <rules>
    <rule>
      <description>This is the pattern for the file title.</description>
      <match>
        <block>
          <elementmatch type="pattern" id="firstline" position="first">MODERN LIBRARY( SERIES)?.*</elementmatch>
        </block>
      </match>
      <action>
        <assignblock>
          <assign matchedId="firstline" path="TITLE" />
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>This is the pattern for the file title.</description>
      <match>
        <block>
          <elementmatch type="pattern" id="firstline" position="first">Modern Library Series.*</elementmatch>
        </block>
      </match>
      <action>
        <assignblock>
          <assign matchedId="firstline" path="TITLE" />
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>This is the pattern for the file title.</description>
      <match>
        <block>
          <elementmatch type="pattern" id="firstline" position="first">Modern Library.*</elementmatch>
        </block>
      </match>
      <action>
        <assignblock>
          <assign matchedId="firstline" path="TITLE" />
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>This is the pattern for the date heading.</description>
      <match>
        <block>
          <elementmatch type="pattern" id="year">(Fall )?\d\d\d\d</elementmatch>
        </block>
      </match>
      <action>
        <assignblock>
          <assign matchedId="year" path="HEAD" />
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>This is the pattern for the season heading.</description>
      <match>
        <block>
          <elementmatch type="pattern" id="season">(Spring)|(Fall)</elementmatch>
        </block>
      </match>
      <action>
        <assignblock>
          <assign matchedId="season" path="HEAD" />
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>
        A rule to match the "Titles sought, suggested, declined" section.
        
        For other sections, we use lenghts longer than 20 characters to differentiate
        headers from paragraphs... since this title is longer, we must apply this rule first or
        it'll get swallowed up by the previous section.
      </description>
      <match>
        <block>
          <elementmatch type="pattern" id="head">Titles sought, suggested, declined\s*</elementmatch>
          <elementmatch qualifier="+" type="pattern" id="paragraph">.{20,}</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="TITLES_S_S_D">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>
        Matches a section that has a paragraph with just "General" followed by
        any number of paragraphs with 20 or more characters in them.  The first
        one is assigned as the heading of the "general" section and each subsequent
        paragraph is marked as a paragraph within the section.
      </description>
      <match>
        <block>
          <elementmatch type="pattern" id="head">General\s*</elementmatch>
          <elementmatch qualifier="+" type="pattern" id="paragraph">.{20,}</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="GENERAL">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>
        Matches a section that has a paragraph with just "Format" followed by
        any number of paragraphs with 20 or more characters in them.  The first
        one is assigned as the heading of the "FORMAT" section and each subsequent
        paragraph is marked as a paragraph within the section.
      </description>
      <match>
        <block>
          <elementmatch type="pattern" id="head">Format\s*</elementmatch>
          <elementmatch qualifier="+" type="pattern" id="paragraph">.{20,}</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="FORMAT">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>
        Matches a section that has a paragraph with just "Number of titles" followed by
        any number of paragraphs with 20 or more characters in them.  The first
        one is assigned as the heading of the "NUMBER_OF_TITLES" section and each subsequent
        paragraph is marked as a paragraph within the section.
      </description>
      <match>
        <block>
          <elementmatch type="pattern" id="head">Number of titles\s*</elementmatch>
          <elementmatch qualifier="+" type="pattern" id="paragraph">.{20,}</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="NUMBER_OF_TITLES">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>
        Matches a section that has a paragraph with just "Title page" followed by
        all content until a paragraph that says "Binding" is found.  The "Title page"
        paragraph is marked as a header in the "TITLE_PAGE" section and all paragraphs
        except the final "Binding" paragraph are marked as paragraphs within that
        section.  This pattern will obviously fail for title page sections not followed
        by binding sections.
      </description>
      <match>
        <block>
          <elementmatch type="pattern" id="head">Title [Pp]age\s*</elementmatch>
          <elementmatch qualifier="+" type="pattern" inverse="true" id="paragraph">Binding\s*</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="TITLE_PAGE">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>
        Matches a section that has a paragraph with just "Binding" followed by
        any number of paragraphs with 20 or more characters in them.  The first
        one is assigned as the heading of the "BINDING" section and each subsequent
        paragraph is marked as a paragraph within the section.
      </description>
      <match>
        <block>
          <elementmatch type="pattern" id="head">Binding\s*</elementmatch>
          <elementmatch qualifier="+" type="pattern" id="paragraph">.{20,}</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="BINDING">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description/>
      <match>
        <block>
          <elementmatch type="pattern" id="head">Endpaper\s*</elementmatch>
          <elementmatch qualifier="+" type="pattern" id="paragraph">.{20,}</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="ENDPAPER">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description/>
      <match>
        <block>
          <elementmatch type="pattern" id="head">Christmas gift box\s*</elementmatch>
          <elementmatch qualifier="+" type="pattern" id="paragraph">.{20,}</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="CHRISTMAS_GIFT_BOX">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description/>
      <match>
        <block>
          <elementmatch type="pattern" id="head">Dating keys?\s*</elementmatch>
          <elementmatch qualifier="+" type="pattern" id="paragraph">.{20,}</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="DATING_KEYS">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description/>
      <match>
        <block>
          <elementmatch type="pattern" id="head">Price\s*</elementmatch>
          <elementmatch type="pattern" id="paragraph">.*</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="PRICE">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description/>
      <match>
        <block>
          <elementmatch type="pattern" id="head">Discontinued( titles)?\s*</elementmatch>
          <elementmatch type="pattern" id="paragraph" qualifier="+">(None\.)|(.*\(\d\d\d\d\)).?</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="DISCONTINUED_TITLES">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description/>
      <match>
        <block>
          <elementmatch type="pattern" id="head">New titles?\s*</elementmatch>
          <elementmatch type="pattern" id="paragraph" qualifier="+">.* \(?\d+\)?</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="NEW_TITLES">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description/>
      <!-- THORNTON WILDER.  THE CABALA.  1929–1937.  ML 155)
           WILLIAM FAULKNER.  ABSALOM, ABSALOM!  1951–  .  (ML 271)
      -->
      <match>
        <block>
          <elementmatch type="pattern" id="number">^\s*\d+\s*$</elementmatch>
          <elementmatch type="pattern" id="head">^\s*(.+)(\.  )(.+)([\.\?!]  ?)(.+)(\.  ?\(?ML\s+)([0-9]+)A?(\))\s*$</elementmatch>
          <elementmatch type="pattern" id="paragraph" qualifier="+" inverse="true">^\s*\d+\s*$</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="BOOK">
          <assign matchedId="number" path="NUMBER"/>
          <assign matchedId="head" capturingGroup="1" path="METADATA/AUTHOR"/>
          <assign matchedId="head" capturingGroup="2" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="3" path="METADATA/TITLE"/>
          <assign matchedId="head" capturingGroup="4" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="5" path="METADATA/DATE_RANGE"/>
          <assign matchedId="head" capturingGroup="6" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="7" path="METADATA/ML_NUMBER"/>
          <assign matchedId="head" capturingGroup="8" path="METADATA/TEXT"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>Matches "EIGHT FAMOUS ELIZABETHAN PLAYS.  1932–1969.  (ML 94)"</description>
      <match>
        <block>
          <elementmatch type="pattern" id="number">^\s*\d+\s*$</elementmatch>
          <elementmatch type="pattern" id="head">^\s*(.+)(\.  )(\d{4}[–\-]\d{4})(\.  ?\(ML\s+)([0-9]+)(\))\s*$</elementmatch>
          <elementmatch type="pattern" id="paragraph" qualifier="+" inverse="true">^\s*\d+\s*$</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="BOOK">
          <assign matchedId="number" path="NUMBER"/>
          <assign matchedId="head" capturingGroup="1" path="METADATA/TITLE"/>
          <assign matchedId="head" capturingGroup="2" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="3" path="METADATA/DATE_RANGE"/>
          <assign matchedId="head" capturingGroup="4" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="5" path="METADATA/ML_NUMBER"/>
          <assign matchedId="head" capturingGroup="6" path="METADATA/TEXT"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>Matches "THE ARABIAN NIGHTS’ ENTERTAINMENTS.  1932–  .  (ML 201)"</description>
      <match>
        <block>
          <elementmatch type="pattern" id="number">^\s*\d+\s*$</elementmatch>
          <elementmatch type="pattern" id="head">^\s*(.+)(\.  )(\d\d\d\d[–\-]\s*)(\.  ?\(ML\s+)([0-9]+)(\))\s*$</elementmatch>
          <elementmatch type="pattern" id="paragraph" qualifier="+" inverse="true">^\s*\d+\s*$</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="BOOK">
          <assign matchedId="number" path="NUMBER"/>
          <assign matchedId="head" capturingGroup="1" path="METADATA/TITLE"/>
          <assign matchedId="head" capturingGroup="2" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="3" path="METADATA/DATE_RANGE"/>
          <assign matchedId="head" capturingGroup="4" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="5" path="METADATA/ML_NUMBER"/>
          <assign matchedId="head" capturingGroup="6" path="METADATA/TEXT"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <!-- TODO COMPLETE RULE BELOW -->
    <rule>
      <description>Matches "WILLIAM SHAKESPEARE.  THE TRAGEDIES OF SHAKESPEARE.  1 vol.  1943–1947.  (ML 1);  2 vols.  1947–1971.  (ML 1, 1A; ML 2, 3)" and "WILLIAM SHAKESPEARE.  THE HISTORIES AND POEMS OF SHAKESPEARE.  1 vol.  1943–1947.  (ML 3);  2 vols.  1947–1971.  (ML 3, 3A; ML 6, 7)"</description>
      <match>
        <block>
          <elementmatch type="pattern" id="number">^\s*\d+\s*$</elementmatch>
          <elementmatch type="pattern" id="head">^\s*(.+)(\.  )(.+)(\.  .*)(\d\d\d\d[–\-](\d\d\d\d)?)(\s*\.  ?\(ML )(\d+)(\);  2 vols.  1947–1971.  \(ML \d, \dA; ML )(\d)(, )(\d)(\)*)$</elementmatch>
          <elementmatch type="pattern" id="paragraph" qualifier="+" inverse="true">^\s*\d+\s*$</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="BOOK">
          <assign matchedId="number" path="NUMBER"/>
          <assign matchedId="head" capturingGroup="1" path="METADATA/AUTHOR"/>
          <assign matchedId="head" capturingGroup="2" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="3" path="METADATA/TITLE"/>
          <assign matchedId="head" capturingGroup="4" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="5" path="METADATA/DATE_RANGE"/>
          <assign matchedId="head" capturingGroup="7" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="8" path="METADATA/ML_NUMBER"/>
          <assign matchedId="head" capturingGroup="9" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="10" path="METADATA/ML_NUMBER"/>
          <assign matchedId="head" capturingGroup="11" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="12" path="METADATA/ML_NUMBER"/>
          <assign matchedId="head" capturingGroup="13" path="METADATA/TEXT"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>Matches "HENRIK IBSEN.  SIX PLAYS.  1957–1973.  (ML 6; ML 305)" and "O. HENRY.  THE BEST SHORT STORIES OF O. HENRY.  1945–  .  (ML 4; ML 26)" and "KATHERINE ANNE PORTER.  FLOWERING JUDAS AND OTHER STORIES.  1940–1950; 1953–1970.  (ML 88; ML 284)"</description>
      <match>
        <block>
          <elementmatch type="pattern" id="number">^\s*\d+\s*$</elementmatch>
          <elementmatch type="pattern" id="head">^\s*(.+)(\.  )(.+)(\.  )(\d\d\d\d[–\-](\d\d\d\d)?)((; )(\d\d\d\d[–\-](\d\d\d\d)?))?(\s*\.  ?\(ML\s+)(\d+)(; ML )(\d+)(\))\s*$</elementmatch>
          <elementmatch type="pattern" id="paragraph" qualifier="+" inverse="true">^\s*\d+\s*$</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="BOOK">
          <assign matchedId="number" path="NUMBER"/>
          <assign matchedId="head" capturingGroup="1" path="METADATA/AUTHOR"/>
          <assign matchedId="head" capturingGroup="2" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="3" path="METADATA/TITLE"/>
          <assign matchedId="head" capturingGroup="4" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="5" path="METADATA/DATE_RANGE"/>
          <assign matchedId="head" capturingGroup="8" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="9" path="METADATA/DATE_RANGE"/>
          <assign matchedId="head" capturingGroup="11" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="12" path="METADATA/ML_NUMBER"/>
          <assign matchedId="head" capturingGroup="13" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="14" path="METADATA/ML_NUMBER"/>
          <assign matchedId="head" capturingGroup="15" path="METADATA/TEXT"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>Matches "BENVENUTO CELLINI.  AUTOBIOGRAPHY OF BENVENUTO CELLINI.  1927–1970; 1985–1991.  (ML 3; 150)"</description>
      <match>
        <block>
          <elementmatch type="pattern" id="number">^\s*\d+\s*$</elementmatch>
          <elementmatch type="pattern" id="head">^\s*(.+)(\.  )(.+)(\.  )(\d\d\d\d[–\-]\d\d\d\d; \d\d\d\d[–\-]\d\d\d\d)(\.  ?\(ML\s+)([\d]+)(; )(\d+)(\))\s*$</elementmatch>
          <elementmatch type="pattern" id="paragraph" qualifier="+" inverse="true">^\s*\d+\s*$</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="BOOK">
          <assign matchedId="number" path="NUMBER"/>
          <assign matchedId="head" capturingGroup="1" path="METADATA/AUTHOR"/>
          <assign matchedId="head" capturingGroup="2" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="3" path="METADATA/TITLE"/>
          <assign matchedId="head" capturingGroup="4" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="5" path="METADATA/DATE_RANGE"/>
          <assign matchedId="head" capturingGroup="6" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="7" path="METADATA/ML_NUMBER"/>
          <assign matchedId="head" capturingGroup="8" path="METADATA/TEXT"/>
          <assign matchedId="head" capturingGroup="9" path="METADATA/ML_NUMBER"/>
          <assign matchedId="head" capturingGroup="10" path="METADATA/TEXT"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
    <rule>
      <description>
        The text of the Jacket section is hard to generalize, 
        so we match it dead last and include anything unmatched 
        after it.  In effect, it will be delimited by other matches.
      </description>
      <match>
        <block>
          <elementmatch type="pattern" id="head">Jackets?\s*</elementmatch>
          <elementmatch qualifier="+" type="pattern" id="paragraph">.*</elementmatch>
        </block>
      </match>
      <action>
        <assignblock path="JACKET">
          <assign matchedId="head" path="HEAD"/>
          <assign matchedId="paragraph" path="PARAGRAPH"/>
        </assignblock>
      </action>
    </rule>
  </rules>

